<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hindustani">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hindu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NumberActivity"
            android:label="Number"
            android:theme="@style/NumberTheme" />
        <activity
            android:name=".FamilyActivity"
            android:label="Family"
            android:theme="@style/FamilyTheme" />
        <activity
            android:name=".PhrasesActivity"
            android:label="Phrases"
            android:theme="@style/PhrasesTheme" />
        <activity
            android:name=".ColorsActivity"
            android:label="Colors"
            android:theme="@style/ColorsTheme" />
    </application>

</manifest>